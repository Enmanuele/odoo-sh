<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	
	<!-- Municipios -->
	<record model="ir.actions.act_window" id="res_municipality_action">
		<field name="name">Municipios</field>
		<field name="res_model">res.municipality</field>
		<field name="view_mode">tree,form</field>
	</record>
	<record id="res_municipality_tree" model="ir.ui.view">
		<field name="name">Municipios</field>
		<field name="model">res.municipality</field>
		<field name="arch" type="xml">
			<tree>
				<field name="code"/>
				<field name="name"/>
				<field name="res_country_state_id"/>
			</tree>
		</field>
	</record>
</odoo>